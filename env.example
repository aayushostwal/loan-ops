# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/kaaj
SYNC_DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/kaaj

# OpenAI API Configuration (for LLM processing)
# Required for both lender processing and loan application matching
OPENAI_API_KEY=your-openai-api-key-here

# Hatchet Configuration (for workflow orchestration)
# Get your token from https://cloud.onhatchet.run/
# Leave empty to run in mock mode (workflows won't be processed)
# Used for both lender processing and loan application matching
HATCHET_CLIENT_TOKEN=

# Tesseract Configuration (optional - only if custom path needed)
# TESSERACT_CMD=/usr/local/bin/tesseract

# Application Settings
LOG_LEVEL=INFO

